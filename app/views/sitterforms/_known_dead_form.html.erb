<% for known_dead in @sitterforms.known_deads %>
  <%= f.fields_for :known_deads, known_dead do |f| %>
    <%= f.label :name %> 
    <%= f.text_field :name %>

    <%= f.label :reltionship_id, "Relationship" %>
    <%= f.select :relationship_id, Relationship.all.collect { |p| [ p.name, p.id ] } %></br>

    <%= f.label :year_of_death %> 
    <%= f.text_field :year_of_death %>
  <% end %>
<% end %>