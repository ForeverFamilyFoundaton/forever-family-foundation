<p id="notice"><%= notice %></p>

<h1>Listing Sitterforms</h1>

<table>
  <thead>
    <tr>
      <th>SitterformID</th>
      <th>UserID</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sitterform.each do |s| %>
      <tr>
        <td><%= s.id %></td>
        <td><%= s.user_id %></td>
        <td><%= s.phone %></td>
        <td><%= link_to 'Show', s %></td>
        <td><%= link_to 'Edit', edit_sitterform_path(s) %></td>
        <td><%= link_to 'Destroy', s, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sitterform', new_sitterform_path %>
